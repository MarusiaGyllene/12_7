per_cent = {'ТКБ': 5.6, 'СКБ': 5.9, 'ВТБ': 4.28, 'СБЕР': 4.0}

money = float(input("Введите сумму: "))

deposit = []
for perst in per_cent.values():
    deposit.append(money*perst/100)

max_depst = max(deposit)
print ("deposit =", deposit)
print ("Максимальная сумма, которую вы можете заработать — ", max_depst)
